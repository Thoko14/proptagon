# PropBase Environment Configuration
# Copy this file to .env and fill in your actual values

# Mapbox Configuration
# Get your access token from: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Optional: Custom Mapbox Style URL
# VITE_MAPBOX_STYLE_URL=mapbox://styles/mapbox/streets-v12

# Development Configuration
NODE_ENV=development

# API Configuration (if using external APIs)
# VITE_API_BASE_URL=http://localhost:8000
# VITE_API_KEY=your_api_key_here

# Feature Flags
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_DEBUG_MODE=true

# Cloudflare Turnstile Configuration
# Get your keys from: https://dash.cloudflare.com/profile/api-tokens
# Site Key (public) - used in frontend
VITE_TURNSTILE_SITE_KEY=your_turnstile_site_key_here
# Secret Key (private) - used in backend Lambda functions
TURNSTILE_SECRET_KEY=your_turnstile_secret_key_here
